{
  "name": "Conference",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nom": {
      "type": "string",
      "required": true
    },
    "dateDebut": {
      "type": "date",
      "required": true
    },
    "dateFin": {
      "type": "date",
      "required": true
    },
    "codeAcces": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "enquetes": {
      "type": "hasMany",
      "model": "Enquete",
      "foreignKey": ""
    },
    "questionnaires": {
      "type": "hasMany",
      "model": "Questionnaire",
      "foreignKey": ""
    },
    "speaker": {
      "type": "belongsTo",
      "model": "Speaker",
      "foreignKey": "speakerId"
    },
    "attachments": {
      "type": "hasMany",
      "model": "Attachment",
      "foreignKey": "conferenceId"
    },
    "questions": {
      "type": "hasMany",
      "model": "Question",
      "foreignKey": "conferenceId"
    }
  },
  "acls": [],
  "methods": {}
}
